      <ComboBox fx:id="cbClasses" layoutX="87.0" layoutY="30.0" onAction="#onClassChoose" prefHeight="25.0" prefWidth="229.0" />
      <GridPane fx:id="grid" layoutX="102.0" layoutY="72.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <Button fx:id="bConfirm" layoutX="165.0" layoutY="338.0" mnemonicParsing="false" onAction="#onConfirmPressAction" text="Confirm" />
   </children>
</AnchorPane>
